<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomFont">
        <attr name="customTypeface" format="string" />
        <attr name="baseTypeface">
            <enum name="regular" value="0"/>
            <enum name="light" value="1"/>
            <enum name="bold" value="2"/>
        </attr>
    </declare-styleable>


    <declare-styleable name="WeekPickerAttrs">
        <attr name="isSelected" format="boolean" />
        <attr name="pickerColor" format="color" />
        <attr name="selectedIndex" format="integer"/>
    </declare-styleable>

    <declare-styleable name="AssessmentRowAttrs">
        <attr name="isFooter" format="boolean" />
        <attr name="isHeader" format="boolean" />
    </declare-styleable>

</resources>